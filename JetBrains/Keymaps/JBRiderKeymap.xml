<keymap version="1" name="MB Rider Keymap" parent="MB Base Keymap">
  <action id="$Cut">
    <keyboard-shortcut first-keystroke="shift delete" />
  </action>
  <action id="$Redo">
    <keyboard-shortcut first-keystroke="ctrl y" />
  </action>
  <action id="ActivateBuildToolWindow">
    <keyboard-shortcut first-keystroke="shift ctrl b" />
  </action>
  <action id="ActivateCommitToolWindow">
    <keyboard-shortcut first-keystroke="alt 0" />
    <keyboard-shortcut first-keystroke="shift alt g" />
    <keyboard-shortcut first-keystroke="alt g" />
  </action>
  <action id="ActivateDebugToolWindow">
    <keyboard-shortcut first-keystroke="shift ctrl y" />
  </action>
  <action id="ActivateNuGetToolWindow">
    <keyboard-shortcut first-keystroke="shift ctrl n" />
  </action>
  <action id="ActivateProjectToolWindow">
    <keyboard-shortcut first-keystroke="meta b" />
    <keyboard-shortcut first-keystroke="ctrl b" />
    <keyboard-shortcut first-keystroke="shift ctrl e" />
  </action>
  <action id="ActivateUnitTestsToolWindow">
    <keyboard-shortcut first-keystroke="shift ctrl t" />
  </action>
  <action id="Back">
    <keyboard-shortcut first-keystroke="meta alt left" />
    <keyboard-shortcut first-keystroke="ctrl alt left" />
  </action>
  <action id="CloseContent">
    <keyboard-shortcut first-keystroke="meta w" />
    <keyboard-shortcut first-keystroke="ctrl w" />
  </action>
  <action id="CloseProject">
    <keyboard-shortcut first-keystroke="ctrl k" second-keystroke="f" />
    <keyboard-shortcut first-keystroke="shift ctrl w" />
    <keyboard-shortcut first-keystroke="meta k" second-keystroke="f" />
  </action>
  <action id="CommentByBlockComment">
    <keyboard-shortcut first-keystroke="shift alt a" />
    <keyboard-shortcut first-keystroke="shift ctrl slash" />
  </action>
  <action id="CommentByLineComment">
    <keyboard-shortcut first-keystroke="meta slash" />
    <keyboard-shortcut first-keystroke="ctrl slash" />
    <keyboard-shortcut first-keystroke="ctrl k" second-keystroke="ctrl c" />
    <keyboard-shortcut first-keystroke="ctrl k" second-keystroke="ctrl u" />
  </action>
  <action id="Editor Copy">
    <keyboard-shortcut first-keystroke="ctrl c" />
  </action>
  <action id="Editor Paste">
    <keyboard-shortcut first-keystroke="ctrl v" />
  </action>
  <action id="Editor Redo">
    <keyboard-shortcut first-keystroke="shift meta z" />
  </action>
  <action id="Editor SelectAll">
    <keyboard-shortcut first-keystroke="ctrl a" />
  </action>
  <action id="Editor Undo">
    <keyboard-shortcut first-keystroke="ctrl z" />
  </action>
  <action id="EditorBackSpace">
    <keyboard-shortcut first-keystroke="back_space" />
  </action>
  <action id="EditorDeleteLine">
    <keyboard-shortcut first-keystroke="shift meta back_space" />
  </action>
  <action id="EditorDeleteToLineStart">
    <keyboard-shortcut first-keystroke="shift back_space" />
    <keyboard-shortcut first-keystroke="meta back_space" />
  </action>
  <action id="EditorDuplicateLines">
    <keyboard-shortcut first-keystroke="shift ctrl d" />
  </action>
  <action id="EditorLineEndWithSelection">
    <keyboard-shortcut first-keystroke="shift end" />
    <keyboard-shortcut first-keystroke="shift meta right" />
  </action>
  <action id="EditorLineStartWithSelection">
    <keyboard-shortcut first-keystroke="shift home" />
    <keyboard-shortcut first-keystroke="shift meta left" />
  </action>
  <action id="EditorNextWord">
    <keyboard-shortcut first-keystroke="ctrl right" />
    <keyboard-shortcut first-keystroke="alt right" />
  </action>
  <action id="EditorNextWordWithSelection">
    <keyboard-shortcut first-keystroke="shift ctrl right" />
    <keyboard-shortcut first-keystroke="shift alt right" />
  </action>
  <action id="EditorPreviousWord">
    <keyboard-shortcut first-keystroke="ctrl left" />
    <keyboard-shortcut first-keystroke="alt left" />
  </action>
  <action id="EditorPreviousWordWithSelection">
    <keyboard-shortcut first-keystroke="shift ctrl left" />
    <keyboard-shortcut first-keystroke="shift alt left" />
  </action>
  <action id="EditorPasteSimple">
    <keyboard-shortcut first-keystroke="shift ctrl alt v" />
    <keyboard-shortcut first-keystroke="shift ctrl v" />
  </action>
  <action id="Find">
    <keyboard-shortcut first-keystroke="ctrl f" />
    <keyboard-shortcut first-keystroke="meta f" />
  </action>
  <action id="FindInPath">
    <keyboard-shortcut first-keystroke="shift ctrl f" />
    <keyboard-shortcut first-keystroke="shift meta f" />
  </action>
  <action id="Forward">
    <keyboard-shortcut first-keystroke="meta alt right" />
    <keyboard-shortcut first-keystroke="ctrl alt right" />
  </action>
  <action id="GotoDeclaration">
    <keyboard-shortcut first-keystroke="f12" />
    <mouse-shortcut keystroke="control button1" />
    <mouse-shortcut keystroke="meta button1" />
  </action>
  <action id="GotoSymbol">
    <keyboard-shortcut first-keystroke="shift ctrl o" />
  </action>
  <action id="MoveLineDown">
    <keyboard-shortcut first-keystroke="ctrl down" />
  </action>
  <action id="MoveLineUp">
    <keyboard-shortcut first-keystroke="ctrl up" />
  </action>
  <action id="MyPlugin.Editor.actions.OpenInCodeSnapAction">
    <keyboard-shortcut first-keystroke="shift alt 4" />
  </action>
  <action id="RecentFiles">
    <keyboard-shortcut first-keystroke="ctrl e" />
    <keyboard-shortcut first-keystroke="meta e" />
  </action>
  <action id="RecentProjectListGroup">
    <keyboard-shortcut first-keystroke="ctrl r" />
    <keyboard-shortcut first-keystroke="meta r" />
  </action>
  <action id="ReplaceInPath">
    <keyboard-shortcut first-keystroke="shift ctrl h" />
    <keyboard-shortcut first-keystroke="shift meta h" />
  </action>
  <action id="ShowIntentionActions">
    <keyboard-shortcut first-keystroke="ctrl period" />
    <keyboard-shortcut first-keystroke="meta period" />
  </action>
  <action id="Vcs.QuickListPopupAction">
    <keyboard-shortcut first-keystroke="ctrl v" />
    <keyboard-shortcut first-keystroke="alt v" />
    <keyboard-shortcut first-keystroke="meta back_quote" />
  </action>
  <action id="one.util.ideaplugin.screenshoter.SaveImageAction">
    <keyboard-shortcut first-keystroke="shift alt 4" />
  </action>
</keymap>