<keymap version="1" name="JBIdeaKeymap" parent="MB Base Keymap">
  <action id="$Cut">
    <keyboard-shortcut first-keystroke="meta x" />
  </action>
  <action id="ActivateCommitToolWindow">
    <keyboard-shortcut first-keystroke="meta 0" />
  </action>
  <action id="ActivateDebugToolWindow">
    <keyboard-shortcut first-keystroke="shift meta y" />
  </action>
  <action id="ActivateProjectToolWindow">
    <keyboard-shortcut first-keystroke="shift meta e" />
    <keyboard-shortcut first-keystroke="meta b" />
  </action>
  <action id="ActivateServicesToolWindow">
    <keyboard-shortcut first-keystroke="meta 8" />
    <keyboard-shortcut first-keystroke="ctrl t" />
  </action>
  <action id="ActivateTerminalToolWindow">
    <keyboard-shortcut first-keystroke="ctrl back_quote" />
  </action>
  <action id="Console.Jdbc.Execute">
    <keyboard-shortcut first-keystroke="meta enter" />
  </action>
  <action id="DatabaseView.DataSourceCutAction">
    <keyboard-shortcut first-keystroke="meta x" />
  </action>
  <action id="EditorDeleteLine">
    <keyboard-shortcut first-keystroke="shift meta k" />
    <keyboard-shortcut first-keystroke="shift delete" />
  </action>
  <action id="FindNext">
    <keyboard-shortcut first-keystroke="f3" />
    <keyboard-shortcut first-keystroke="meta k" second-keystroke="meta d" />
  </action>
  <action id="FindPrevious">
    <keyboard-shortcut first-keystroke="shift f3" />
    <keyboard-shortcut first-keystroke="shift meta f5" />
  </action>
  <action id="QuickJavaDoc">
    <mouse-shortcut keystroke="control button2" />
    <keyboard-shortcut first-keystroke="meta k" second-keystroke="meta i" />
    <keyboard-shortcut first-keystroke="shift meta space" />
  </action>
  <action id="SelectAllOccurrences">
    <keyboard-shortcut first-keystroke="meta f2" />
    <keyboard-shortcut first-keystroke="shift meta l" />
  </action>
  <action id="UsageView.Remove">
    <keyboard-shortcut first-keystroke="meta x" />
  </action>
  <action id="Vcs.Push">
    <keyboard-shortcut first-keystroke="meta p" />
  </action>
</keymap>